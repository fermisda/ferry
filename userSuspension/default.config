[log]
level=info
file=userSuspension.log

[ssl]
cert=/etc/grid-security/jobsub/fifebatch-hostcert.pem
key=/etc/grid-security/jobsub/fifebatch-hostkey.pem
cadir=/etc/grid-security/certificates

[ferry]
hostname=https://ferrydev.fnal.gov:8443/
api=getAllUsersFQANs?suspend=true

[condor]
hold_command=/usr/bin/condor_hold -constraint '%s'
